{
  "name": "makerx-components",
  "version": "1.0.0",
  "description": "Components used across Microsoft MakerX experiences.",
  "main": "dist/index.js",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./jestsetup.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/.*((test|spec)\\.jsx?)|((test|spec)\\.tsx?))$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-options": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "@types/jest": "21.1.1",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/storybook__react": "^3.0.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "jest": "21.2.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "storybook-addon-props": "^3.0.4",
    "ts-jest": "21.0.1",
    "ts-loader": "^3.5.0",
    "tslint": "5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "@business-app/fabric": "^0.9.0",
    "@types/react-modal": "^3.1.2",
    "@types/react-router-dom": "^4.2.6",
    "office-ui-fabric-core": "^9.6.0",
    "office-ui-fabric-react": "^5.89.0",
    "react": "^16.2.0",
    "react-awesome-modal": "^2.0.3",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.4",
    "react-model": "0.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.15.1"
  },
  "author": "Microsoft Corporation",
  "license": "ISC"
}
